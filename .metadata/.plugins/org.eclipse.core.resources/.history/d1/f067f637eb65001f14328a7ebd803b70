package exercio_4;

public class Exercio_4 {

	public static void main(String[] args) {
		
		double a =  80000;
		double b = 200000;
		
		for(int i = 0;a < b;i++) {
			
			a = 80000 * 1.03;
			b = 200000 * 1.015;
			System.out.println("entrei em for");
			System.out.println("anos "+i);
		}
		
		System.out.println("acabou o for");
	}

}
